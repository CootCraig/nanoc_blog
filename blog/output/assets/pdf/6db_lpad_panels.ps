%!PS-Adobe-2.0
%%Creator: Craig Anderson in vim
%%Title: 3db lpad panels
%%Pages: 1
%%PageOrder: Ascend
%%BoundingBox: 0 0 612 792
%%DocumentPaperSizes: Letter
%%EndComments

/inch {72 mul} def

/rca_diameter { 0.25 inch } def
/rca_spacing { 11 16 div inch } def

/marker_diameter { 0.05 inch } def

/rca_panel_width { 2.36 inch } def
/rca_panel_height { 0.893 inch } def

/switch_diameter { 5.0 32.0 div inch } def
/switch_panel_width { 2.185 inch } def
/switch_panel_height { 1.75 inch } def

/rca_panel {
  newpath

  0 0 moveto
  rca_panel_width 0 rlineto
  0 rca_panel_height rlineto
  rca_panel_width neg 0 rlineto
  closepath stroke

  rca_panel_width 2 div
  rca_panel_height 2 div

  gsave
    rca_spacing 2 div neg 0 translate

    newpath
    rca_panel_width 2 div
    rca_panel_height 2 div
    rca_diameter 2 div
    0 360 arc
    closepath stroke

    newpath
    rca_panel_width 2 div
    rca_panel_height 2 div
    marker_diameter 2 div
    0 360 arc
    closepath fill
  grestore
  gsave
    rca_spacing 2 div 0 translate

    newpath
    rca_panel_width 2 div
    rca_panel_height 2 div
    rca_diameter 2 div
    0 360 arc
    closepath stroke

    newpath
    rca_panel_width 2 div
    rca_panel_height 2 div
    marker_diameter 2 div
    0 360 arc
    closepath fill
  grestore


} def

/switch_panel {
  newpath
  0 0 moveto
  switch_panel_width 0 rlineto
  0 switch_panel_height rlineto
  switch_panel_width neg 0 rlineto
  closepath stroke

  newpath
  switch_panel_width 2 div
  switch_panel_height 2 div
  switch_diameter 2 div
  0 360 arc
  closepath stroke

  newpath
  switch_panel_width 2 div
  switch_panel_height 2 div
  marker_diameter 2 div
  0 360 arc
  closepath fill

  newpath
  switch_panel_width 2 div
  switch_panel_height 2 div
  0.457 inch add
  marker_diameter 2 div
  0 360 arc
  closepath fill

} def

gsave
  0.5 inch 0.5 inch translate
  rca_panel

  0 inch 1.25 inch translate
  rca_panel
grestore

gsave
  3.5 inch 0.5 inch translate
  switch_panel
grestore

showpage

